@using StorForStudentsWebApp.Models
@model AdminCatalogViewModel

@{
    ViewBag.Title = "Редактор товаров.";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Панель администратора. Редактор товаров.</h2>

<table class="table table-striped table-hover">
    <tr>
        <th>
            Название товара
        </th>
        <th>
            Цена
        </th>
        <th>
            Количество
        </th>
    </tr>

@foreach (ItemModel item in Model.itemList) {
    <tr>
        <td>
            @item.Name
        </td>
        <td>
            @item.Price
        </td>
        <td>
            @item.Quantity
        </td>
    </tr>
}
</table>
